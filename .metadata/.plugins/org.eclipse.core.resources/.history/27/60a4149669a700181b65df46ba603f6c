package Test2;

public class Zero {
	public static void main(String[] s) {
		int arr[]={5,8,0,1,4,0,3,9,7,0};
		int len=arr.length;
		int temp;
		int zero_index;
		for(int i=0;i<len;i++) {
			temp=arr[i];
			if(temp==0) {
				if(arr[i+1]!=0) {
					arr[i]=arr[i+1];
					arr[i+1]=temp;
				}
				else {
					continue;
				}
			}
		}
	}
}
