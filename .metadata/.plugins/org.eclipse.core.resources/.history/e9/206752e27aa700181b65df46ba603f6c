
public class Initblock {
	public static void main(String a[]) {
		int i=300;
		float f=200.6573f;
//		System.out.println(f);
//		double l=45.12345542454222323243260;
//		System.out.println("Long="+l);
		Integer inte=new Integer(123);
		System.out.println(inte);
		Float ae=12.98798f;
		Double qe=12121.12122;
		System.out.println(qe.getClass().getName());
		System.out.println(ae.getClass().getName());
		
	}
}
