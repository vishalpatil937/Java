package Test2;

public class Test2 {
	public static boolean contains(String[] array,String item) {
		for(String ite:array) {
			if(ite==item || ite.equals(item))
				return true;
		}
		return false;
	}
	
	public static void main(String s[]) {
		String str[]= {"a","b","c"};
		System.out.println(contains(str,"c"));
	}
	
	
}


