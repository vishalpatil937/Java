package Test2;

public class HappyNumber {
	public static void main(String a[]) {
		int number=19;
		happy();
		
	}
	static void happy() {
		int number=19;
		int sum=0;
		while(number>0) {
			sum+=(int) Math.pow(number%10, 2);
			System.out.println();
			number/=10;
		}
		System.out.println(sum);
	}
}	
