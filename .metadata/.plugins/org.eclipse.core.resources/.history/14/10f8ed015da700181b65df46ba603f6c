package Test2;

public class Duplicate_elem {
	static int visited[];
	static int[] corect_array;
	public static void main(String a[]) {
		int arr[]= {214,921,552,180,900,125,124,584,214};
		visited=new int[arr.length];
		correct_array=new int[arr.length];
		for(int i=0;i<arr.length;i++)
			visited[i]=0;
		System.out.println("The initial length of array is: "+arr.length);
		
	}
	public static int remove_dupl(int arr[]) {
		int temp;
		for(int i=0;i<arr.length;i++) {
			temp=arr[i];
			visited[i]=1;
			if(contains(arr,arr[i])!=-1) {
				
			}
		}
	}
	
	public static int contains(int arr[],int elem) {
		int index=-1;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]==elem && visited[i]!=1 ) {
				index=i;
				visited[i]=1;
				break;
			}
		}
		return index;
	}
	
}

